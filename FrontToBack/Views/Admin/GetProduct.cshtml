
@{
    ViewData["Title"] = "GetProduct";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model List<Product>
<div class="container bg-white p-5">
    <table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>OldPrice</th>
            <th>NewPrice</th>
            <th>ImageUrl</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
           <tr>
               <td>@item.Name</td>
               <td>@item.OldPrice</td>
               <td>@item.NewPrice</td>
               <td><img width="100" src="~/shop/img/@item.ImageUrl" /></td>
           </tr> 
        }
    </tbody>
</table>
</div>

